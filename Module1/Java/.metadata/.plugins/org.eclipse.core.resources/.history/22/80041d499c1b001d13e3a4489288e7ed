package lab1;

class problem1{
	problem1(){}
	public int calculateSum(int n) {
		int sum=0;
		for(int i=1;i<=n;i++) {
			if(i%3==0 || i%5==0) {
				sum=sum+i;
			}
		}
		return sum;
	}
}
public class Lab1 {


	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		problem1 P1 = new problem1();
		System.out.print(P1.calculateSum(10));
	}

}
