package labExercises;

class problem41{
	public problem41() {}
	public int sumOfCubes(int n) {
		char digits[] = String.valueOf(n).toCharArray();
		int result=0;
		for(int i=0;i<digits.length-1;i++) {
			int dig=(int) digits[i];
			result = (int) (result+ Math.pow(dig, 3));
	}
}
