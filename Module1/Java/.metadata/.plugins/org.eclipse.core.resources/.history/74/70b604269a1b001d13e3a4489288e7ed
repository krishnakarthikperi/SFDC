package lab1;

int calculateSum(int n) {
	int sum=0;
	for(int i=1;i<=n;i++) {
		if(i%3==0 || i%5==0) {
			sum=sum+i;
		}
	}
	return sum;
}

public class Lab1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print(calculate(10));
	}

}
